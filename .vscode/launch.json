{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "CIS195 Windows Launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/TestAnyLab.mjs",
            "args": [
                "G:/My Drive/Courses/CIS195/2023-Fall/Labs/CIS195Lab06GradeAssist.csv"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "CIS195 MacOS Launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/TestAnyLab.mjs",
            "args": [
                "Temp/CIS195TermProjectGradeAssist.csv"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "CS133JS MacOS Launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            /*
            "program": "${workspaceFolder}/TestAnyLab.mjs",
            "args": [
                "/Volumes/GoogleDrive/My Drive/Courses/CS133JS/23F/Labs/CS133JS_Lab01GradeAssist.csv",
                "--overwrite"
            ]
            */
            "program": "${workspaceFolder}/TestAnyLab.mjs",
            "args": [
                "/Volumes/GoogleDrive/My Drive/Courses/CS133JS/24W/CS133JSW24Lab05/CS133JS_Lab05A_GradeAssist.csv",
                "--overwrite"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "CS133JS Windows Launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/TestAnyLab.mjs",
            "args": [
                "G:/My Drive//Courses/CS133JS/24W/CS133JSW24Lab05/CS133JS_Lab05A_GradeAssist.csv",
                "--overwrite"
            ]
        },
        {
            "type": "node-terminal",
            "name": "JavaScript Debug Terminal",
            "request": "launch",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Mocha Tests",
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "args": [
                "--no-timeouts",
                "--colors",
                "${workspaceFolder}/test"
